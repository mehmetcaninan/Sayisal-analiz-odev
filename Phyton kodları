import math

# ğœ‹/5 deÄŸerini belirle
x = math.pi / 5

cos_x = math.cos(x)

tek = 1
  
iki = 1 - (x**2) / 2

kesme_tek = abs(cos_x - tek)
kesme_iki = abs(cos_x - iki)

# SonuÃ§larÄ± yazdÄ±r
print(f"GerÃ§ek cos(ğœ‹/5) deÄŸeri: {cos_x}")
print(f"Taylor serisinin 1 terimle hesaplanmÄ±ÅŸ hali: {tek}")
print(f"Taylor serisinin 2 terimle hesaplanmÄ±ÅŸ hali: {iki}")
print(f"1 terimle hesaplanan kesme hatasÄ±: {kesme_tek}")
print(f"2 terimle hesaplanan kesme hatasÄ±: {kesme_iki}")
